<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="@dimen/shape_item_border_height"
                android:height="@dimen/shape_item_border_height"/>
            <stroke
                android:width="2dp"
                android:color="@color/shape_item_border_color"/>
            <corners
                android:radius="@dimen/shape_item_border_rounding"/>
        </shape>
    </item>
</layer-list>
